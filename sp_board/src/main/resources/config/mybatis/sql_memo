/*MariaDB */


CREATE TABLE `tbl_board` (
	`bno` INT(11) NOT NULL AUTO_INCREMENT,
	`title` VARCHAR(200) NOT NULL,
	`content` TEXT NULL,
	`writer` VARCHAR(50) NULL,
	`regdate` TIMESTAMP NULL DEFAULT CURRENT_TIMESTAMP,
	`viewcnt` INT(11) NULL,
	PRIMARY KEY (`bno`)
)
COLLATE='utf8_general_ci'
;